{
  "name": "ens-hackathon-tools",
  "version": "1.0.0",
  "description": "ENS Hackathon Analysis Tools",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "install:all": "npm install",
    "build": "npm run build --workspaces",
    "build:analyzer": "npm run build --workspace=@ens-hackathon/analyzer",
    "test": "npm run test --workspaces",
    "test:analyzer": "npm run test --workspace=@ens-hackathon/analyzer",
    "lint": "npm run lint --workspaces",
    "clean": "npm run clean --workspaces",
    "publish": "lerna publish",
    "dev:analyzer": "npm run dev --workspace=@ens-hackathon/analyzer",
    "analyze": "node packages/analyzer/dist/cli/index.js"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "lerna": "^7.4.2",
    "typescript": "^5.2.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1"
  },
  "keywords": ["ens", "ethereum", "hackathon", "analysis", "web3", "monorepo"],
  "author": "ENS Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ensdomains/ens-hackathon-tools.git"
  }
}